//command to find data from last or first word

select * from teacher where name like '%d';

select * from teacher where name like 'd%';

//command for find on 2nd char:
select * from teacher where name like '_d%';

//command for find on 2nd char:
 select * from teacher where name like '__i%';

//group of city where is u char and its count: 
select city,count(city) from teacher group by city having city like '%u%';
